<?xml version="1.0" encoding="UTF-8"?>
<Report xmlns="http://soap.sforce.com/2006/04/metadata">
    <aggregates>
        <calculatedFormula>CDF1:SUM/Opportunity.ACV__c.CONVERT:SUM</calculatedFormula>
        <datatype>percent</datatype>
        <developerName>FORMULA1</developerName>
        <isActive>true</isActive>
        <isCrossBlock>false</isCrossBlock>
        <masterLabel>% Won within 90 Days</masterLabel>
        <scale>2</scale>
    </aggregates>
    <aggregates>
        <calculatedFormula>(
CDF1:SUM +
PREVGROUPVAL(CDF1:SUM, CLOSE_MONTH,1)  +
PREVGROUPVAL(CDF1:SUM, CLOSE_MONTH,2)
)/3</calculatedFormula>
        <datatype>number</datatype>
        <developerName>FORMULA2</developerName>
        <downGroupingContext>CLOSE_MONTH</downGroupingContext>
        <isActive>false</isActive>
        <isCrossBlock>false</isCrossBlock>
        <masterLabel>Rolling 3 Mo. AVG (SUM)</masterLabel>
        <scale>2</scale>
    </aggregates>
    <aggregates>
        <calculatedFormula>(
(CDF1:SUM/Opportunity.ACV__c.CONVERT:SUM) +
(PREVGROUPVAL(CDF1:SUM,CLOSE_MONTH,1)/PREVGROUPVAL(Opportunity.ACV__c.CONVERT:SUM, CLOSE_MONTH,1))  +
(PREVGROUPVAL(CDF1:SUM,CLOSE_MONTH,2)/PREVGROUPVAL(Opportunity.ACV__c.CONVERT:SUM,CLOSE_MONTH,2))
)/3</calculatedFormula>
        <datatype>number</datatype>
        <developerName>FORMULA3</developerName>
        <downGroupingContext>CLOSE_MONTH</downGroupingContext>
        <isActive>false</isActive>
        <isCrossBlock>false</isCrossBlock>
        <masterLabel>Rolling 3 Mo. AVG (%)</masterLabel>
        <scale>2</scale>
    </aggregates>
    <aggregates>
