    <customDetailFormulas>
        <calculatedFormula>IF(text(STAGE_NAME)=&quot;Closed Won&quot;,
    (IF(Opportunity.SAL_Acceptance_Date__c&lt;&gt;NULL,
        (IF(CLOSE_DATE-Opportunity.SAL_Acceptance_Date__c&lt;=90,Opportunity.ACV__c.CONVERT,0)),
